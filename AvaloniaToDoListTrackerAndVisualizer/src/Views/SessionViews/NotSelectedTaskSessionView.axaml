<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:AvaloniaToDoListTrackerAndVisualizer.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:SessionViewModel"
             x:Class="AvaloniaToDoListTrackerAndVisualizer.Views.SessionViews.NotSelectedTaskSessionView">
    <Button Command="{Binding SelectTaskCommand}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="5" CornerRadius="20" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Content="Select task" FontSize="17"></Button>
</UserControl>
